#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/user/Documents/GitHub/syafe.github.io/node_modules/d3-dsv/bin/node_modules:/mnt/c/Users/user/Documents/GitHub/syafe.github.io/node_modules/d3-dsv/node_modules:/mnt/c/Users/user/Documents/GitHub/syafe.github.io/node_modules:/mnt/c/Users/user/Documents/GitHub/node_modules:/mnt/c/Users/user/Documents/node_modules:/mnt/c/Users/user/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/user/Documents/GitHub/syafe.github.io/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/user/Documents/GitHub/syafe.github.io/node_modules/d3-dsv/bin/node_modules:/mnt/c/Users/user/Documents/GitHub/syafe.github.io/node_modules/d3-dsv/node_modules:/mnt/c/Users/user/Documents/GitHub/syafe.github.io/node_modules:/mnt/c/Users/user/Documents/GitHub/node_modules:/mnt/c/Users/user/Documents/node_modules:/mnt/c/Users/user/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/user/Documents/GitHub/syafe.github.io/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../d3-dsv/bin/json2dsv.js" "$@"
else
  exec node  "$basedir/../d3-dsv/bin/json2dsv.js" "$@"
fi
